{
  "name": "jabranr/postcodes-io",
  "description": "PHP library for postcodes.io API",
  "keywords": [
    "postcode",
    "UK",
    "address"
  ],
  "type": "library",
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Jabran Rafique",
      "email": "hello@jabran.me"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">= 7.3"
  },
  "scripts": {
    "test": [
      "composer validate",
      "phpunit"
    ]
  },
  "autoload": {
    "psr-4": {
      "Jabranr\\PostcodesIO\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jabranr\\PostcodesIO\\Tests\\": "tests"
    }
  }
}
